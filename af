        <script type="text/javascript">

            // declare variable
            var scrollTop = $(".scrolltoTop");

            $(window).scroll(function () {
                // declare variable
                var topPos = $(this).scrollTop();

                // if user scrolls down - show scroll to top button
                if (topPos > 100) {
                    $(scrollTop).css("opacity", "1");

                } else {
                    $(scrollTop).css("opacity", "0");
                }

            }); // scroll END



            //Click event to scroll to top
            $(scrollTop).click(function () {
                $('html, body').animate({
                    scrollTop: 0
                }, 600);
                return false;

            }); // click() scroll top EMD
        </script>

        <script>
            $(":checkbox").on("change", function () {
                $(this).parent().toggleClass("checked", this.checked);

            });

            $('.nexttab').click(function () {
                $('#gamingtabs .nav-tabs > .nav-item > .active').parent().next('li').find('a').trigger('click');
            });
            $('.previous').click(function () {
                $('#gamingtabs .nav-tabs > .nav-item > .active').parent().prev('li').find('a').trigger('click');
            });


            //force reloading a page when using browser back button
            if (performance.navigation.type == 2) {
                location.reload(true);
            }
            //
            $("input.LCheckbox").change(function () {
                var maxAllowed = 3;
                var cnt = $("input.LCheckbox:checked").length;
                if (cnt > maxAllowed) {
                    $(this).prop("checked", "");

                    $msg = 'You can select maximum of 3 games!';
                    $(".alert-msg").html($msg);
                    $("#GamesAlertBtn").trigger("click");
                    $(this).parent().toggleClass("checked", this.checked);

                }
                if (cnt == 0) {
                    $("#gamenext1-1").show();
                }
            });
            // Show next button
            var $submit = $("#gamenext1").hide(),
                $cbs = $('input.LCheckbox').click(function () {
                    $("#gamenext1-1").hide();
                    $submit.toggle($cbs.is(":checked"));
                });
            $("#gamenext1-1").click(function () {
                $msg = "You have to select minimum of 1 game!";
                $(".alert-msg").html($msg);
                $("#GamesAlertBtn").trigger("click");
            });
            // Show next button
            $("#gamenext2").hide();
            $(".resCheckbox").change(function () {
                $(".resCheckbox").prop('checked', false);
                $(".resolution-wrap label").removeClass('checked');
                $labelid = $(this).val();
                $(".resolution-wrap label#" + $labelid + "").addClass('checked');


                $(this).prop('checked', true);
                $("#gamenext2-1").hide();
                $("#gamenext2").show();
            });
            $("#gamenext2-1").click(function () {
                //alert();
                $msg = "You have to select minimum of 1 resolution!";
                $(".alert-msg").html($msg);
                $("#GamesAlertBtn").trigger("click");
            });
            // Show Submit button
            $("#gamenext3").hide();
            $(".platformCheckbox").change(function () {
                $(".platformCheckbox").prop('checked', false);

                $(".brand-wrap label").removeClass('checked');
                $labelid = $(this).val();
                $(".brand-wrap label#" + $labelid + "").addClass('checked');

                $(this).prop('checked', true);
                $("#gamenext3-1").hide();
                $("#gamenext3").show();
            });
            $("#gamenext3-1").click(function () {
                //alert();
                $msg = "You have to select minimum of 1 platform!";
                $(".alert-msg").html($msg);
                $("#GamesAlertBtn").trigger("click");
            });
        </script>



        <script type="text/javascript">
            // document ready
            $(document).ready(function () {

                // init ez-slider with slick
                $('.ez-slider').on('init', function (slick) {

                    // slide var for use timeout
                    const slider = this;

                    // slight delay so slick init completes render
                    setTimeout(function () {

                        // init the easy zoom on slider 
                        $('FIGURE', slider).addClass('easyzoom').easyZoom();

                        // thumb buttons
                        let thumbs = $('.slick-dots > LI > BUTTON', slider);

                        // each thumbnail button function
                        $.each(thumbs, function (i, e) {

                            // slide id
                            let slide_id = $(this).attr('aria-controls');

                            // get thumbnail image src
                            let thumb_img = $('#' + slide_id).find('IMG').attr('src');

                            // change thumb button inner html too
                            $(this).html('<img src="' + thumb_img + '" alt="" />');

                        });

                    }, 100);

                    // then our slick options
                }).slick({
                    slidesToShow: 1,
                    slidesToScroll: 1,
                    dots: true,
                    arrows: false,
                    adaptiveHeight: true,
                    asNavFor: '.ez-slider .slick-dots',
                    autoplay: true,

                });





                $('.ez-slider .slick-dots')
                    .on('init', function (slick) {
                        $('.ez-slider .slick-dots').fadeIn(1000);
                        const slider = $('.ez-slider');
                        const thumbsClass = $(this).find('.slick-list li button')
                        setTimeout(function () {
                            // thumb buttons
                            let thumbs = thumbsClass;


                            $.each(thumbs, function (i, e) {
                                let slide_id = $(this).attr('aria-controls');
                                let thumb_img = $('#' + slide_id).find('IMG').attr('src');
                                // change thumb button inner html too
                                $(this).html('<img src="' + thumb_img + '" alt="" />');
                                $(this).on('click', function () {
                                    // Move aria-current="true" to this button                
                                    // Change the main image to match this thumbnail button
                                    var index = $(this).closest('.slick-slide').data('slick-index');
                                    $('.ez-slider').slick('slickGoTo', index);
                                });

                            });

                        }, 100);
                    })
                    .slick({
                        slidesToShow: 4,
                        slidesToScroll: 1,
                        asNavFor: '.ez-slider',
                        dots: false,
                        arrows: true,
                        centerMode: false,
                        focusOnSelect: false,
                        autoplay: true,
                        focusOnChange: false
                    });

                // On before slide change
                $('.ez-slider .slick-dots').on('beforeChange', function (event, slick, currentSlide, nextSlide) {
                    console.log(nextSlide);
                });

                $('.ez-slider .slick-dots').on('afterChange', function (e, slick, currentSlide, direction) {
                    if (currentSlide) {
                        console.log(currentSlide)
                        slick.slickPrev();
                    }
                });

                $('.ez-slider .slick-dots').slick('slickPrev');



            });
        </script>

