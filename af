<script>
    function goforcart(id) {
        var product_id = id;
        $(this).find('.spinner-border').fadeIn();
        $.ajax({
            type: "POST",
            url: "https://Need4uNeed4u.in/home/ajaxaddtocart",
            data: { rowid: product_id },
            success: function (res) {
                if (res > 0) {
                    /*location.reload();*/
                    $('.count-style').html(res);
                }
            }
        });
    }
    function gpForSubValidate() {
        $filter = /^([a-z0-9_\.\-])+\@(([a-z0-9\-])+\.)+([a-z0-9]{2,4})+$/;
        $emailid = $('#emailid').val();
        if (!$filter.test($emailid)) {
            $("#emailid").css({ "border": "solid 1px red" });
            return false;
        }
    }

</script>





<script>
    // AJAX call for autocomplete 
    $(document).ready(function () {
        $("#search-box").keyup(function () {
            $('.serachLoader').show();

            $.ajax({
                type: "POST",
                url: "https://Need4uNeed4u.in/search-suggestion",
                data: 'keyword=' + $(this).val(),
                beforeSend: function () {
                    //$("#search-box").css("background","#FFF url(LoaderIcon.gif) no-repeat 165px");
                },
                success: function (data) {
                    $("#suggesstion-box").show();
                    $("#suggesstion-box").html(data);
                    //$("#search-box").css("background","#FFF");
                    $('.serachLoader').hide();
                }
            });
        });
    });

    $(document).ready(function () {
        $("#suggesstion-box-lg-overlay").hide();
        $("#search-box-lg").keyup(function () {

            $('.serachLoader').show();

            $.ajax({
                type: "POST",
                url: "https://Need4uNeed4u.in/search-suggestion",
                data: 'keyword=' + $(this).val(),
                beforeSend: function () {
                    //$("#search-box").css("background","#FFF url(LoaderIcon.gif) no-repeat 165px");
                },
                success: function (data) {
                    $("#suggesstion-box-lg").show();
                    $("#suggesstion-box-lg-overlay").show();
                    $("#suggesstion-box-lg").html(data);
                    //$("#search-box").css("background","#FFF");

                    $('.serachLoader').hide();

                }
            });
        });

        $("#suggesstion-box-lg-overlay").click(function () {
            $("#suggesstion-box-lg-overlay").hide();
            $("#suggesstion-box-lg").html('');
        });

    });

//To select country name
